
<!DOCTYPE html>
<html class="no-js" lang="en">
<head>


    @RenderSection("CSS", false)

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dicle-Demir</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*<img src="~/Uploads/Sliders/iconfinder_Psychology_3436581.png" style="margin-top: -24px" />*@

    <link rel="icon" href="~/Uploads/Sliders/iconfinder_Psychology_3436581.png" />
    <!-- Place favicon.ico in the root directory -->
    <!-- all css here -->
    <!-- bootstrap.min.css -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
            integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
            crossorigin="anonymous"></script>
    @*<script src="http://maps.google.com/maps/api/js?key=AIzaSyCZ3R3HV2ECPiClKyFX8Y_UMFg6T_2L1e0"></script>*@
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <!-- font-awesome.min.css -->
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <!-- owl.carousel.min.css -->
    <link rel="stylesheet" href="~/Content/css/owl.carousel.css">
    <!-- animate.css -->
    <link rel="stylesheet" href="~/Content/css/animate.css">
    <!-- meanmenu.min.css -->
    <link rel="stylesheet" href="~/Content/css/meanmenu.min.css">
    <!-- shortcodes.css -->
    <link rel="stylesheet" href="~/Content/css/shortcode/shortcodes.css">

    <!-- style.css -->
    <link href="~/Content/style.css" rel="stylesheet" />

    <!-- responsive.css -->
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <!-- modernizr-js -->
    <script src="~/Content/js/vendor/modernizr-2.8.3.min.js"></script>
</head>
<body>

    <div id="preloader"></div>
    <header>
        <div class="header-area white-bg" id="main_h">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-8">
                        <div class="logo">
                            
                                <img src="~/Uploads/Sliders/iconfinder_Psychology_3436581.png" style="margin-top: -24px" />
                           

                                @*<a href="index.html"><img src="~/Content/img/logo.png" alt="" /></a>*@
</div>
                    </div>
                    <div class="col-lg-7 hidden-sm hidden-xs col-md-7">
                        <div class="mainmenu">
                            <nav>
                                <ul>
                                    <li> @Html.ActionLink("AnaSayfa", "Index", "Home") </li>
                                    <li> @Html.ActionLink("Randevu", "Index", "Appointment")  </li>

                                    @Html.Action("_Article", "Article")


                                    <li><a href="about.html">Hakkımızda</a></li>
                                    <li>@Html.ActionLink("İletişim", "Contact", "Home")</li>
                                </ul>
                            </nav>
                        </div>
                    </div>

                    <div class="col-lg-2 col-md-2 col-sm-4">
                        <div class="mainmenu">

                            <ul class="nav navbar-nav navbar-right">

                                @if (User.Identity.IsAuthenticated)
                                {

                                    if (User.IsInRole("Administrator"))
                                    {
                                        <li class="nav navbar-right">
                                            @Html.ActionLink("Çıkış", "LogOff", "Members")

                                        </li>

                                                <li class="nav navbar-right">
                                                    <a href="/AdminContact/MyAccount">Profilim<i class="fa fa-caret-down"></i></a>

                                                    <ul>
                                                        <li>
                                                            @Html.ActionLink("Şifreni Değiştir", "ChangePass", "Members")
                                                        </li>
                                                        <li>
                                                            <a href="/AdminContact/MyAccount">Sayfam </a>
                                                        </li>
                                                        <li>
                                                            @Html.ActionLink("Slider Kontrol Et", "Slider", "Home")
                                                        </li>
                                                        <li>
                                                            @Html.ActionLink("Randevuları Yönet", "Index", "AdminAppointment")
                                                        </li>
                                                        <li>
                                                            @Html.ActionLink("Mesajları Yönet", "Index", "AdminContact")
                                                        </li>
                                                        <li>
                                                            @Html.ActionLink("Makale Oluştur", "CreateArticle", "AdminArticle")
                                                        </li>
                                                        <li>
                                                            @Html.ActionLink("Kategori Oluştur", "CreateCategory", "AdminArticle")
                                                        </li>
                                                    </ul>
                                                </li>
                                    }
                                    else
                                    {

                                        <li class="nav navbar-right"> @Html.ActionLink("Çıkış", "LogOff", "Members") </li>

                                            <li class="nav navbar-right">
                                                <a href="/Members/MyAccount">Profilim<i class="fa fa-caret-down"></i></a>
                                                <ul>
                                                    <li>
                                                        @Html.ActionLink("Şifreni Değiştir", "ChangePass", "Members")
                                                    </li>
                                                    <li>
                                                        <a href="/Members/MyAccount">  Sayfam </a>
                                                    </li>
                                                </ul>
                                            </li>
                                    }
                                }

                                else
                                {
                                    <li class="nav navbar-right">@Html.ActionLink("Giriş", "Login", "Members")</li>
                                            <li class="nav navbar-right">@Html.ActionLink("Kayıt Ol", "Register", "Members")</li>
                                }
                            </ul>

                        </div>
                    </div>
                </div>
            </div>




            <!-- mobile-menu-area-start -->
            <div class="mobile-menu-area visible-xs visible-sm">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="mobile-menu">
                                <nav id="mobile-menu">

                                    <ul>
                                        <li> @Html.ActionLink("AnaSayfa", "Index", "Home") </li>
                                        <li> @Html.ActionLink("Randevu", "Index", "Appointment")  </li>

                                        @Html.Action("_Article", "Article")
                                        <li><a href="about.html">Hakkımızda</a></li>
                                        <li>@Html.ActionLink("İletişim", "Contact", "Home")</li>
                                    </ul>

                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- mobile-menu-area-end -->
        </div>
    </header>
    <!-- header-end -->
    <!-- page-header-area-start -->
    <div class="page-header-area ptb-140">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="page-header-text text-center text-white">
                        <h2 class="page-title">Hoşgeldiniz</h2>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <ul class="breadcrumb">
                                <li> @User.Identity.Name </li>

                            </ul>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- page-header-area-end -->
    <!-- post-content-area-start -->
    <div class="post-content-area ptb-80">
        @RenderBody()



    </div>
    <!-- post-content-area-end -->
    <!-- instagram-area-start -->
    <div class="instagram-area">
        <div class="container-fluid">
            <div class="row">
                <div class="instagram-wrap">
                    <div class="instagram-img-wrap">
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/1.jpg" alt="Responsive Image"></a>
                        </div>
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/2.jpg" alt="Responsive Image"></a>
                        </div>
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/3.jpg" alt="Responsive Image"></a>
                        </div>
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/4.jpg" alt="Responsive Image"></a>
                        </div>
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/1.jpg" alt="Responsive Image"></a>
                        </div>
                        <div class="col-xs-12 col-sm-2 pn">
                            <a href="#"><img src="~/Content/img/instagram/5.jpg" alt="Responsive Image"></a>
                        </div>
                    </div>
                    <div class="instagram-caption">
                        <div class="instagram-caption-inside">
                            @*<div class="instagram-caption-content">
                                    <ul class="details-link">
                                        <li><a href="#">Follow us @@Instagram</a></li>
                                    </ul>
                                </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- instagram-area-end -->
    <!-- brand-area-start -->
    <!-- brand-area-end -->
    <!-- footer-start -->
    <!-- footer-end -->
    <!-- copyright-area-start -->
